<!-- Modal -->
<div id="LogOut" tabindex="-1" role="dialog" aria-labelledby="LogOut" aria-hidden="true">
  <div role="document">
    <div>
      <div>
        <h5 id="exampleModalLongTitle"><%= current_user.name %> info</h5>
        <ion-icon name="arrow-back" data-dismiss="modal" aria-label="Close"></ion-icon>
      </div>
      <div>If you log out, you will be logged out of all your devices. 
      <%= link_to 'Log out', destroy_user_session_path, method: :delete, class: 'btn btn-danger'%>
      </div>
    </div>
  </div>
</div>
